{
  "name": "thread-utils",
  "version": "1.0.0",
  "scripts": {
    "build": "tsc",
    "watch": "npx nodemon --exec 'npm run build' -e .ts --watch lib-src",
    "task:create-sandbox-perms": "node -r esm ./utils/create-test-permutations.js",
    "pretest:dedupe": "rm -f sandbox/index.html",
    "test:dedupe": "node -r esm ./utils/dedupe-images.js",
    "sandbox": "npx serve sandbox"
  },
  "engines": {
    "node": ">14"
  },
  "esm": {
    "await": true,
    "cache": false
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "esm": "^3.2.25",
    "imghash": "^0.0.8",
    "lodash": "^4.17.21",
    "minimist": "^1.2.5",
    "pg": "^8.5.1",
    "sequelize": "^6.5.1",
    "sharp": "^0.27.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/node": "^14.14.31",
    "prettier": "^2.2.1",
    "typescript": "^4.2.3"
  }
}
